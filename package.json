{
    "name": "emacs-ctrl-l",
    "displayName": "Emacs Ctrl+L",
    "description": "Emacs-style Ctrl+L recenter behavior",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.93.1"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "emacs-ctrl-l.recenterCursor",
                "title": "Recenter Cursor (Emacs style)"
            }
        ],
        "keybindings": [
            {
                "command": "emacs-ctrl-l.recenterCursor",
                "key": "ctrl+l",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "devDependencies": {
        "@types/node": "^14.x.x",
        "@types/vscode": "^1.93.1",
        "typescript": "^4.4.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/gtech/vscode-emacs-ctrl-l.git"
    },
    "publisher": "gtech"
}
